{{define "API-Delete"}}
  Future<bool> delete{{.Model.Name}}({{.Model.PrimaryField.Type}} {{.Model.PrimaryField.Name}}) async {
    final response = await client.delete(
      "$baseUrl{{.API.URL}}/${{.Model.PrimaryField.Name}}",
      headers: {"content-type": "application/json"},
    );
    if (response.statusCode == 200) {
      return true;
    } else {
      return false;
    }
  }
   {{end}}