{{define "API-Find"}}
Future<{{.Model.Name}}> get{{.Model.Name}}({{.Model.PrimaryField.Type}} {{.Model.PrimaryField.Name}}) async {
    final response = await client.get( "$baseUrl{{.API.URL}}/${{.Model.PrimaryField.Name}}");
    if (response.statusCode == 200) {
      return {{.Model.Name}}FromJson(response.body);
    } else {
      return null;
    }
  }
  {{end}}