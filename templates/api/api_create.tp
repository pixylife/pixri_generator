  {{define "API-Create"}}
  Future<bool> create{{.Model.Name}}({{.Model.Name}} data) async {
    final response = await client.post(
      "$baseUrl{{.API.URL}}",
      headers: {"content-type": "application/json"},
      body: {{.Model.Name}}ToJson(data),
    );
    if (response.statusCode == 200) {
      return true;
    } else {
      return false;
    }
  }
 {{end}}